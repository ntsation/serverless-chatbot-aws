{
    "version": "2017-02-28",
    "operation": "Query",
    "query": {
        "expression": "chatId = :chatId",
        "expressionValues": {
            ":chatId": $util.dynamodb.toDynamoDBJson($context.arguments.chatId)
        }
    },
    "scanIndexForward": true,
    "limit": $util.defaultIfNull($context.arguments.limit, 20)
}
